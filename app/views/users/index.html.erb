<div class="container">
    <h1>Find buddies</h1>
    <p>path is : users_path </p>
<%= render 'components/user_search' %>

    <% @users.each do |user| %>
    <div class="projects-list">
        <p class="mt-3"><strong>
            <%= link_to user.first_name, user_path(user.id) %>
            <%= link_to user.last_name, user_path(user.id) %>
        </strong></p>
        <p> <%= user.personal_summary %></p>
    </div>
    <%  end %>
</div>
